{
	"name": "web_lic",
	"version": "0.47.0",
	"description": "Create an instruction book from a 3D model",
	"main": "src/ui.js",
	"scripts": {
		"build": "webpack --config webpack.config.js --config-name local",
		"prod": "webpack --config webpack.config.js --config-name prod",
		"start": "webpack-dev-server --open --config webpack.config.js --config-name local",
		"test": "npx cypress open",
		"deploy": "python deploy.py",
		"stats": "webpack --config webpack.prod.js --profile --json > stats.json",
		"profile": "webpack-bundle-analyzer stats.json"
	},
	"keywords": [],
	"author": "Remi Gagne",
	"homepage": "http://bugeyedmonkeys.com",
	"license": "GPL-3.0-or-later",
	"devDependencies": {
		"babel-polyfill": "^6.26.0",
		"css-loader": "0.28.11",
		"cypress": "3.1.5",
		"eslint": "5.1.0",
		"eslint-loader": "2.1.0",
		"eslint-plugin-vue": "4.7.0",
		"lodash": "^4.17.11",
		"uglifyjs-webpack-plugin": "1.2.4",
		"vue-loader": "15.2.4",
		"vue-style-loader": "4.1.0",
		"vue-template-compiler": "2.5.16",
		"webpack": "4.1.1",
		"webpack-bundle-analyzer": "2.13.1",
		"webpack-cli": "2.0.12",
