		"css-loader": "0.28.11",
		"cypress": "3.5.0",
		"eslint": "5.1.0",
		"eslint-loader": "2.1.0",
		"eslint-plugin-no-only-tests": "2.3.1",
		"eslint-plugin-vue": "6.0.0",
		"jszip": "3.2.2",
		"lodash": "4.17.15",
		"typescript": "3.7.2",
